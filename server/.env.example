# Production Environment Variables Template
# Copy this file to .env and fill in your values

NODE_ENV=production
PORT=5000

# MongoDB Atlas Connection String
MONGO_URI=mongodb+srv://username:password@cluster.mongodb.net/libraryManagement?retryWrites=true&w=majority

# Frontend URL (Vercel deployment URL)
FRONTEND_URL=https://your-app.vercel.app

# JWT Configuration (use a strong random secret in production)
JWT_SECRET=generate_a_random_64_character_string_here
JWT_EXPIRE=24h
